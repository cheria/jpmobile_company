<!DOCTYPE html>
<html>
<head>
  <title>Company</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="header">
    <%= render :partial => "layouts/header" %>
  </div>
  <div id="wrapper">
    <div id="menu">
      <%= render :partial => "layouts/menu" %>
    </div>
    <div id="contents"><%= yield %></div>
    <div class="clear"></div>
  </div>
  <div id="footer">
    <%= render :partial => "layouts/footer" %>
  </div>
</body>
</html>
